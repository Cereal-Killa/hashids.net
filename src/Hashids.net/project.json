{
  "name":  "Hashids.net",
  "title": "Hashids.net",
  "version": "1.2.0",
  "description": "Generate YouTube-like hashes from one or many numbers. Use hashids when you do not want to expose your database ids to the user.",
  "language": "en-US",
  "packOptions": {
    "requireLicenseAcceptance": false,
    "iconUrl": "https://raw.githubusercontent.com/ullmark/hashids.net/master/hashids.png",
    "releaseNotes": "https://github.com/ullmark/hashids.net#changelog",
    "tags": [ "ids", "hash" ]
  },

  "frameworks": {
    "net40": {
      "buildOptions": {
        "define": ["NET"]
      }
    },

    "net45": {
      "buildOptions": {
        "define": ["NET"]
      }
    },

    "netstandard1.0": {
      "buildOptions": {
        "define": [ "CORE" ]
      },
      "dependencies": {
        "NETStandard.Library": "1.6.0"
      }
    }

  }
}